        <Grid.RowDefinitions>
            <RowDefinition Height="87*"/>
            <RowDefinition Height="243*"/>
        </Grid.RowDefinitions>
        <Frame Name="Imya" Margin="0,91,0,0" Height="359" VerticalAlignment="Top" Grid.RowSpan="2"/>
        <StackPanel Grid.Row="0" Background="Aqua" Orientation="Horizontal">
            <Image Margin="0,0,705,348" Source=".//Source/unknown.png" Stretch="Fill"/>
